{
  "name": "servidor",
  "version": "1.0.0",
  "description": "TypeScript Express.js server with Supabase integration",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "echo 'Linting not configured yet'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-release": "npm run build && npm run start",
    "build-and-start": "npm run build && npm run start"
  },
  "keywords": [
    "typescript",
    "express",
    "supabase",
    "backend",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.19.2",
    "@supabase/supabase-js": "^2.43.1",
    "dotenv": "^16.4.5",
    "cors": "^2.8.5",
    "winston": "^3.12.0",
    "express-ws": "^5.0.2",
    "express-fileupload": "^1.5.0"
  },
  "devDependencies": {
    "typescript": "^5.4.5",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.8",
    "@types/cors": "^2.8.17",
    "@types/express-fileupload": "^1.5.0",
    "@types/express-ws": "^3.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}